{
  "name": "xgplayer-flv-vod",
  "version": "2.5.3",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js && cross-env NODE_ENV=production rollup -c rollup.config.mobile.js ",
    "build:es": "babel src --out-dir es",
    "watch:es": "babel src --out-dir es --watch",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "watch:lib": "rm -rf lib && babel src --out-dir lib --presets=es2015 --watch",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch & npm run watch:lib & npm run watch:es",
    "watch:mobile": "cross-env NODE_ENV=development rollup -c rollup.config.mobile.js --watch"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 58,
            "ie": 10
          },
          "modules": false
        }
      ]
    ]
  },
  "dependencies": {
    "xgplayer-helper-models": "2.5.3",
    "xgplayer-helper-codec": "2.5.3",
    "xgplayer-helper-utils": "2.5.3",
    "xgplayer-helper-transmuxers": "2.5.3"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
