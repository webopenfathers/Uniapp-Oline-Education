{
  "name": "xgplayer-flv",
  "version": "2.5.3",
  "description": "xgplayer transmuxer for FLV",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.min.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd && npm run build:es && npm run build:lib",
    "build:umd": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:es": "babel src --out-dir es",
    "build:lib": "babel src --out-dir lib --presets=es2015",
    "watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/xgplayer.git"
  },
  "keywords": [
    "flv",
    "player",
    "video",
    "live"
  ],
  "author": "fuyuhao@bytedance.com",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "peerDependency": {
    "xgplayer": "^2.3.5"
  },
  "dependencies": {
    "xgplayer-flv-live": "2.5.3",
    "xgplayer-flv-vod": "2.5.3"
  },
  "devDependencies": {
    "cross-env": "^6.0.3"
  }
}
